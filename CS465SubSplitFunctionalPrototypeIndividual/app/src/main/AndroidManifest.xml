<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.illinois.cs465.cs465_subsplit_functionalprototype_individual">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CS465SubSplitFunctionalPrototypeIndividual">
        <activity android:name=".IndividualSubscription"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity"/>
<!--        <activity android:name=".AddMemberActivity"/>-->
        <activity android:name=".AddMemberActivity" android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".CommunityBoardMain" android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".DMBoard" android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".GroupSubscription" android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".IndDMView" android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".Subscriptions" android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".Profile" android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>